{
  "name": "nsu-stevnebehandler",
  "version": "2.0.0",
  "author": "Thomas Laaver <tholav@gmail.com>",
  "description": "Resultat- og stevne- behandler",
  "private": true,
  "repository": {
    "type": "git",
    "url": "github:skruf/nsu"
  },
  "main": "main.js",
  "scripts": {
    "build:staging": "vue-cli-service build --mode staging",
    "build:prod": "vue-cli-service build --mode production",

    "web:dev": "vue-cli-service serve",
    "web:dev:test": "VUE_APP_SEED=false vue-cli-service serve",
    "web:dev:cy": "cypress open",
    "web:test:e2e": "vue-cli-service test:e2e",

    "web:pack:staging": "yarn run build:staging",
    "web:pack:prod": "yarn run build:prod",

    "app:dev": "concurrently 'vue-cli-service serve' 'electron .'",
    "app:pack:staging": "yarn run build:staging && yarn run app:pack",
    "app:pack:prod": "yarn run build:prod && yarn run app:pack",

    "app:deploy:linux": "yarn run build:prod && yarn run app:publish -- --linux",
    "app:deploy:mac": "yarn run build:prod && yarn run app:publish -- --mac",
    "app:deploy:win": "yarn run build:prod && yarn run app:publish -- --win",

    "app:pack": "electron-builder --dir",
    "app:publish": "electron-builder --publish always",
    "app:postinstall": "electron-builder install-app-deps",

    "test:unit": "vue-cli-service test:unit",
    "clean": "rimraf ./bundle ./build ./cache",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@sentry/electron": "^1.3.0",
    "dayjs": "^1.8.23",
    "electron-log": "^4.1.0",
    "electron-updater": "^4.2.5",
    "electron-window-state": "^5.0.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "lodash.range": "^3.2.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqby": "^4.7.0",
    "material-design-icons-iconfont": "^5.0.1",
    "pouchdb-adapter-idb": "^7.2.1",
    "rxdb": "^8.8.0",
    "rxjs": "^6.5.4",
    "uuid": "^7.0.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.5",
    "vue-router": "^3.1.6",
    "vue-toasted": "^1.1.28",
    "vuetify": "^2.2.17",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.1",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "@vue/cli-plugin-babel": "^4.2.3",
    "@vue/cli-plugin-e2e-cypress": "^4.2.3",
    "@vue/cli-plugin-eslint": "^4.2.3",
    "@vue/cli-plugin-router": "^4.2.3",
    "@vue/cli-plugin-typescript": "^4.2.3",
    "@vue/cli-plugin-unit-jest": "^4.2.3",
    "@vue/cli-plugin-vuex": "^4.2.3",
    "@vue/cli-service": "^4.2.3",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.32",
    "babel-plugin-component": "^1.1.1",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.2.0",
    "electron": "^8.1.1",
    "electron-builder": "^22.4.1",
    "electron-reload": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "^4.0.2",
    "flush-promises": "^1.0.2",
    "pouchdb-adapter-memory": "^7.2.1",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "tailwindcss": "^1.2.0",
    "typescript": "^3.8.3",
    "vue-cli-plugin-i18n": "^0.6.1",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  }
}
