{
  "name": "Stevnebehandler",
  "version": "1.1.0",
  "author": "Thomas Laaver <tholav@gmail.com>",
  "description": "Resultat- og stevne- behandler",
  "private": true,
  "repository": {
    "type": "git",
    "url": "github:skruf/nsu"
  },
  "main": "main.js",
  "scripts": {
    "serve": "cross-env-shell vue-cli-service serve",
    "build": "cross-env-shell vue-cli-service build",
    "dev": "cross-env NODE_ENV=development RUN_ENV=app concurrently 'yarn run serve' 'electron .'",
    "dev:web": "cross-env NODE_ENV=development RUN_ENV=web yarn run serve",
    "pack:web": "cross-env NODE_ENV=production RUN_ENV=web yarn run build",
    "pack:app": "cross-env-shell NODE_ENV=production RUN_ENV=app 'yarn run build && yarn run pack'",
    "publish:linux": "cross-env-shell NODE_ENV=production RUN_ENV=app 'yarn run build && yarn run publish -- --linux'",
    "publish:mac": "cross-env-shell NODE_ENV=production RUN_ENV=app 'yarn run build && yarn run publish -- --mac'",
    "publish:win": "cross-env-shell NODE_ENV=production RUN_ENV=app 'yarn run build && yarn run publish -- --win'",
    "test:unit": "cross-env NODE_ENV=test vue-cli-service test:unit",
    "test:e2e2": "cross-env NODE_ENV=test vue-cli-service test:e2e",
    "test:e2e": "cross-env NODE_ENV=test cypress open",
    "pack": "electron-builder --dir",
    "publish": "electron-builder --publish always",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf ./bundle ./build ./cache",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@sentry/electron": "^1.1.0",
    "core-js": "^3.6.0",
    "electron-log": "^4.0.0",
