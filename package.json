    "lodash.merge": "^4.6.2",
    "lodash.pickby": "^4.6.0",
    "lodash.range": "^3.2.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqby": "^4.7.0",
    "material-design-icons-iconfont": "^5.0.1",
    "pouchdb-adapter-idb": "^7.1.1",
    "roboto-fontface": "*",
    "rxdb": "^8.8.0",
    "rxjs": "^6.5.3",
    "uuid": "^3.3.3",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.3",
    "vue-moment": "^4.0.0",
    "vue-router": "^3.1.3",
    "vuetify": "^2.1.15",
    "vuex": "^3.1.2"
  },
