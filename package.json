{
  "name": "Stevnebehandler",
  "version": "1.1.0",
  "author": "Thomas Laaver <tholav@gmail.com>",
  "description": "Resultat- og stevne- behandler",
  "private": true,
  "repository": {
    "type": "git",
    "url": "github:skruf/nsu"
  },
  "main": "main.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "dev": "cross-env NODE_ENV=development RUN_ENV=app concurrently 'npm run serve' 'electron .'",
    "dev:web": "cross-env NODE_ENV=development RUN_ENV=web npm run serve",
    "pack:web": "cross-env NODE_ENV=production RUN_ENV=web npm run build",
    "pack:app": "cross-env-shell NODE_ENV=production RUN_ENV=app 'npm run build && npm run pack'",
    "publish:linux": "cross-env-shell NODE_ENV=production RUN_ENV=app 'npm run build && npm run publish -- --linux'",
    "publish:mac": "cross-env-shell NODE_ENV=production RUN_ENV=app 'npm run build && npm run publish -- --mac'",
    "publish:win": "cross-env-shell NODE_ENV=production RUN_ENV=app 'npm run build && npm run publish -- --win'",
    "tests": "jest --verbose ./src/app",
    "tests:e2e": "cypress open",
    "tests:watch": "jest --watch",
    "tests:coverage": "jest --verbose --coverage",
    "pack": "electron-builder --dir",
    "publish": "electron-builder --publish always",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rimraf ./bundle ./build ./cache"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1",
    "@sentry/electron": "^1.0.0",
    "electron-log": "^3.0.9",
    "electron-updater": "^4.2.0",
    "electron-window-state": "^5.0.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "lodash.pickby": "^4.6.0",
    "pouchdb-adapter-idb": "^7.1.1",
    "rxdb": "^8.7.3",
    "rxjs": "^6.5.3",
    "uuid": "^3.3.3",
    "vue-i18n": "^8.15.0",
    "vue-moment": "^4.0.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.12.7",
    "@types/uuid": "^3.4.6",
    "@types/vue-i18n": "^7.0.0",
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-e2e-cypress": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-typescript": "^4.0.0",
    "@vue/cli-plugin-unit-jest": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "electron": "^7.1.1",
    "electron-builder": "^22.1.0",
    "electron-reload": "^1.5.0",
    "element-ui": "^2.12.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "flush-promises": "^1.0.2",
    "pouchdb-adapter-memory": "^7.1.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "tailwindcss": "^1.1.3",
    "typescript": "~3.5.3",
    "vue-cli-plugin-i18n": "^0.6.0",
    "vue-template-compiler": "^2.6.10"
  }
}
